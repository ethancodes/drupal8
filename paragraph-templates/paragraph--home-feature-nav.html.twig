{#
  For the Paragraph Type: Home Feature Nav
  Fields:
    Feature Navs
  Feature Navs are rendered by paragraph--feature-nav
  #}
{%
  set classes = [
    'home-feature-nav',
    'py-5',
    'paragraph',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished'
  ]
%}
{% block paragraph %}
  <section{{ attributes.addClass(classes) }}>
    <div class="container">
      <div class="card-group d-sm-block d-md-flex">
    {% block content %}
      {{ content }}
    {% endblock %}
      </div>
    </div>
  </section>
{% endblock paragraph %}